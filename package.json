{
  "name": "dinero-bank-manager",
  "version": "0.10.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/www",
    "build": "npm run compile",
    "build:ts": "tsc",
    "compile": "npm run build:ts",
    "prettier:check": "prettier --check \"./**/*.{js,ts,tsx,json,scss,md}\"",
    "prettier:write": "prettier --write \"./**/*.{js,ts,tsx,json,scss,md}\"",
    "pack": "node ./pack.js",
    "test": "jest --no-cache",
    "test:jest": "npm run tests -- --coverage --no-cache",
    "test:watch": "npm run tests -- --watch --no-cache",
    "test:cover:watch": "npm run tests -- --coverage --watch --no-cache",
    "up": "node ./upload.js",
    "release": "node ./release.js",
    "patch": "./scripts/bump-patch.sh",
    "minor": "./scripts/bump-minor.sh",
    "major": "./scripts/bump-major.sh"
  },
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "<rootDir>/test_coverage_dir",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "roots": [
      "<rootDir>",
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "@root/(.*)": "<rootDir>/$1",
      "@utils/(.*)": "<rootDir>/src/utils/$1",
      "@models/(.*)": "<rootDir>/src/models/$1",
      "@controllers/(.*)": "<rootDir>/src/controllers/$1",
      "@mock/(.*)": "<rootDir>/src/test/mock/$1"
    },
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "moduleDirectories": [
      ".",
      "src",
      "src/util",
      "node_modules"
    ],
    "setupFiles": [
      "<rootDir>/jest-setup/index.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/dist"
    ],
    "testRegex": ".*\\.(spec|test)\\.(ts|js)$",
    "transform": {
      "^.+\\.(ts|tsx)?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules"
    ]
  },
  "dependencies": {
    "@types/jest": "^26.0.22",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "grunt": "^1.1.0",
    "grunt-bump": "^0.8.0",
    "grunt-cli": "^1.3.2",
    "guid-typescript": "^1.0.9",
    "http-errors": "^1.7.3",
    "https": "^1.0.0",
    "jest": "^26.6.3",
    "module-alias": "^2.2.2",
    "moment": "^2.24.0",
    "multiparty": "^4.2.1",
    "password-hash": "^1.2.2",
    "prettier": "^1.18.2",
    "ts-postgres": "^1.1.0",
    "tsc": "^1.20150623.0",
    "winston": "^3.2.1",
    "zip-folder": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.11.1",
    "ts-jest": "^26.4.4",
    "tslint": "^5.8.0",
    "typescript": "^4.2.3"
  },
  "_moduleAliases": {
    "@root": "dist",
    "@src": "dist/src",
    "@models": "dist/src/models",
    "@routes": "dist/src/routes",
    "@utils": "dist/src/utils",
    "@controllers": "dist/src/controllers"
  }
}
